<h1>Users</h1>
<ul>
	<% @users.each do |user| %>
		<li>
			<% if user.name.present? %>
				<%= link_to user.name, user_path(user) %> -
			<% end %>
			<%= user.email %> =>
			<%= edit_link(edit_user_path(user)) %> - 
			<%= delete_link(user_path(user)) %>
		</li>
	<% end %>
</ul>
<%= link_to "New user", new_user_path %>